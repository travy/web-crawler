{
    "name": "travy/web-crawler",
    "description": "A web crawler which will traverse links found in all parent and child web pages running and performs tasks on each page.",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "5.7"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WebCrawler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebCrawler\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Travis Anthony Torres",
            "email": "travis@travistorres.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "paquettg/php-html-parser": "^1.7",
        "guzzlehttp/guzzle": "^6.3"
    }
}
